{
	"name": "@mobisys/query-string",
	"version": "6.2.0",
	"description": "Parse and stringify URL query strings",
	"license": "MIT",
	"repository": "mobisysgmbh/query-string",
	"author": {
		"name": "Sefa Ilkimen",
		"email": "sefa.ilkimen@mobisys.de",
		"url": "ilkimen.net"
	},
	"scripts": {
    "clean": "shx rm -rf bundle lib lib-esm",
		"build-cjs": "tsc",
		"build-esm": "tsc -m es6 --outDir lib-esm",
		"build-bundle": "webpack",
		"build": "npm run clean && npm run build-cjs && npm run build-esm && npm run build-bundle",
    "test": "npm run build && ava",
    "prepublishOnly": "npm run test"
	},
  "main": "lib/query-string.js",
  "files": [
    "bundle",
    "lib",
    "lib-esm"
  ],
	"keywords": [
		"browser",
		"querystring",
		"query",
		"string",
		"qs",
		"param",
		"parameter",
		"url",
		"parse",
		"stringify",
		"encode",
		"decode",
		"searchparams"
	],
	"devDependencies": {
		"ava": "^0.25.0",
		"awesome-typescript-loader": "5.2.1",
		"deep-equal": "^1.0.1",
		"fast-check": "^1.5.0",
		"shx": "0.3.2",
		"typescript": "3.1.6",
		"webpack": "4.24.0",
		"webpack-cli": "3.1.2"
	}
}
